    "underscore": "^1.9.2",
    "webpack": "^4.41.2",
    "webpack-stream": "^5.2.1",
    "wrapper-webpack-plugin": "^2.1.0"
  },
  "bundleDependencies": [],
  "license": "MIT",
  "engine": {
    "node": ">=0.8"
  }
}
