{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noStrictGenericChecks": true,
    "target": "es5",
    "sourceMap": true,
    "esModuleInterop": true,
    "strict": true,
    "allowJs": true,
    "noEmit": true
  },
  "exclude": [
    "../../lib"
  ]
}
